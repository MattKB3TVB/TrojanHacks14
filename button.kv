#kivy 1.1.1

<ButtonGame>:
    canvas:
        Color:
            rgb: 1.0, 1.0, 1.0
        Rectangle:
            pos: self.center_x-5, 0
            size: 10, self.height

        Label:
            font_size: 70
            center_x: root.width/2
            top: root.top - 50
            text: "Rage Buttons"

        
